<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.xj.hookdemo">

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".rxjava.RxJavaHookActivity"
            android:exported="false" />
        <activity
            android:name=".thread.TestThreadHookActivity"
            android:exported="false" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".TestOnClickActivity" />
        <activity android:name=".activityhook.TestHookStartActivity" />
        <activity android:name=".TestNotificationActivity" />
        <activity android:name=".activityhook.TestActivityStart" />
        <activity android:name=".activityhook.TestApplicationStart" />
        <activity android:name=".activityhook.TestIActivityManagerStart" />
        <activity android:name=".activityhook.TestClipboardActivity" />
        <activity android:name=".activityhook.EmptyActivity" />
        <activity android:name=".activityhook.TestStartActivityNoRegister" />
    </application>

</manifest>